package com.address.exceptions;

public class AddressGlobaExceptionHandler extends Exception {

	public AddressGlobaExceptionHandler() {
		// TODO Auto-generated constructor stub
	}

	public AddressGlobaExceptionHandler(String message) {
		super(message);
		// TODO Auto-generated constructor stub
	}

	public AddressGlobaExceptionHandler(Throwable cause) {
		super(cause);
		// TODO Auto-generated constructor stub
	}

	public AddressGlobaExceptionHandler(String message, Throwable cause) {
		super(message, cause);
		// TODO Auto-generated constructor stub
	}

	public AddressGlobaExceptionHandler(String message, Throwable cause, boolean enableSuppression,
			boolean writableStackTrace) {
		super(message, cause, enableSuppression, writableStackTrace);
		// TODO Auto-generated constructor stub
	}

}
